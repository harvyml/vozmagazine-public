{
	"presets": [
		"@babel/preset-env","@babel/preset-react"
    ],
    "env": {
        "production": {
            "presets": ["minify"]
        }
    },
    "plugins": [
        ["@babel/plugin-proposal-class-properties"],
        ["@babel/plugin-transform-runtime",
          {
            "regenerator": true
          }
        ]
      ],
    "ignore": [
        "node_modules",
        "pulse-editor/node_modules",
        "/node_modules/"
    ]
} 